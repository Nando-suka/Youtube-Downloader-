# YouTube API Keys
# Get your API key from: https://console.cloud.google.com/apis/credentials
YOUTUBE_API_KEY_MAIN=your_main_api_key_here
YOUTUBE_API_KEY_BACKUP_1=your_backup_api_key_1_here
YOUTUBE_API_KEY_BACKUP_2=your_backup_api_key_2_here
YOUTUBE_API_KEY_BACKUP_3=your_backup_api_key_3_here
YOUTUBE_API_KEY_BACKUP_4=your_backup_api_key_4_here
YOUTUBE_API_KEY_BACKUP_5=your_backup_api_key_5_here

# yt-dlp executable path
# For Windows: yt-dlp.exe (or full path)
# For Linux/Mac: yt-dlp (or full path)
YTDLP_PATH=yt-dlp.exe

# Server Configuration
SERVER_PORT=8080

# Rate Limiting
RATE_LIMIT_REQUESTS=100
RATE_LIMIT_WINDOW=60

# Temporary directory for downloads
TEMP_DIR=./tmp
